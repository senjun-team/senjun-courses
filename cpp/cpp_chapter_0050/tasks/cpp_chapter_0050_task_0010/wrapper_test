#include "ut.hpp"
#include "test_helpers.h"

int main()
{
    using namespace boost::ut;

    "check user input"_test = [] {
        std::string user_input = hlp::remove_spaces(R"(#INJECT-b585472fa)");
        std::sort(user_input.begin(), user_input.end());
        expect(user_input == "23");
    };
}
