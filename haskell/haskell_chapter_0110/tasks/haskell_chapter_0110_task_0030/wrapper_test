import os.path
import sys

plan = '''"You are here: 34.7, 10.1"
'''

s = r"""
#INJECT-ESCAPED-b585472fa
"""

if __name__ == "__main__":
    if not os.path.isfile(sys.argv[1]):
        print(f"Empty console output. Expected:\n{plan}")
        exit(1)

    with open(sys.argv[1]) as f:
        fact = f.read()

    if fact.find(plan) == -1:
        print(f"Invalid console output. Expected:\n{plan}")
        exit(1)
        
    s = s.replace(" ", "")

    if s.find('formatLocation::(Double,Double)->String') == -1:
        print("Please declare 'formatLocation' function")
        exit(1)
