if 's_descr' not in locals():
    print("There is no `s_descr` variable")
    exit(1)

if type(s_descr) is not str:
    print("Variable 's_descr' is not a string")
    exit(1)

if s_descr != "short string":
    print("Variable 's_descr' has unexpected value")
    exit(1)


