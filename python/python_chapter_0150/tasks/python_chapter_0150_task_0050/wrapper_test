from io import StringIO
import sys


old_stdout = sys.stdout
sys.stdout = mystdout = StringIO()

#INJECT-b585472fa

sys.stdout = old_stdout

if "merge_max" not in globals():
    print("Function merge_max() doesn't exist")
    exit(1)

if not callable(merge_max):
    print("'merge_max' is not a function")
    exit(1)

tests = [[ {}, {}, {}], 
         [{1: 1}, {1: 1}, {1: 1}],
         [{1: 1}, {1: 2}, {1: 2}],
         [{1: 3}, {1: 2}, {1: 3}],
         [{1: 3, 2: 1, 4: 0, 5: 1}, {2: 0, 6: 1, 4: 1}, {1: 3, 2: 1, 4: 1, 5: 1, 6: 1}],
         ]

for test in tests:
    d1, d2, plan = test

    fact = merge_max(d1, d2)

    if fact != plan:
        print(f"Error in solution for input: {d1}, {d2}. Expected: {plan}")
        exit(1)


