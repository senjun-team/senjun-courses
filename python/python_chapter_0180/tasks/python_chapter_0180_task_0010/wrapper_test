from io import StringIO
import sys

old_stdout = sys.stdout
sys.stdout = mystdout = StringIO()

#INJECT-b585472fa

sys.stdout = old_stdout

if mystdout.getvalue() != "<class 'type'>\n":
    print("print() output isn't correct")
    exit(1)

text = """
#INJECT-ESCAPED-b585472fa
"""

if text.replace(" ", "").find("type(type)") == -1:
    print("There is no call to type() function for 'type' argument")
    exit(1)