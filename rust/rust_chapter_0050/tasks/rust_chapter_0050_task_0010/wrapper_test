#INJECT-b585472fa

#[derive(Debug)]
enum TestResult {
    WrongResult
}

fn main() -> Result<(), TestResult> {

    let test_arrays = vec![
        vec![7, 3, 11, 9, 250],
        vec![11, 7, 3, 9, 250],
        vec![7, 3, 9, 250, 11],
        vec![8, -1, 141, -1, 2, -1, 154, -1, -3],
        vec![10, 10, 10, 10, 8, 4, 30, 20],
        vec![8, 4, 10, 10, 10, 10, 30, 20],
        vec![8, 4, 30, 20, 10, 10, 10, 10],
        vec![5, 0, 0, 0, 0, 0, 0, 0],
        vec![0, 0, 0, 5, 0, 0, 0, 0],
        vec![0, 0, 0, 0, 0, 0, 0, 5],
        vec![4, 4, 4, 4, 4, 4, 4, 4],
        vec![3, 4, 53, 140, 3, 2, 20, 154],
        vec![-10, -10, -10, -10, 4, 4, -10, -10, -10, 7, -10, -10, -10],
        vec![10],
        vec![-16],
        vec![]
    ];

    let test_values = vec![
        11,
        11,
        11,
        -1,
        10,
        10,
        10,
        0,
        0,
        0,
        4,
        -10,
        -10,
        -10,
        -16,
        0
    ];

    let expected = [
        vec![7, 3, 9, 250, 11],
        vec![7, 3, 9, 250, 11],
        vec![7, 3, 9, 250, 11],
        vec![8, 141, 2, 154, -3, -1, -1, -1, -1],
        vec![8, 4, 30, 20, 10, 10, 10, 10],
        vec![8, 4, 30, 20, 10, 10, 10, 10],
        vec![8, 4, 30, 20, 10, 10, 10, 10],
        vec![5, 0, 0, 0, 0, 0, 0, 0],
        vec![5, 0, 0, 0, 0, 0, 0, 0],
        vec![5, 0, 0, 0, 0, 0, 0, 0],
        vec![4, 4, 4, 4, 4, 4, 4, 4],
        vec![3, 4, 53, 140, 3, 2, 20, 154],
        vec![4, 4, 7, -10, -10, -10, -10, -10, -10, -10, -10, -10, -10],
        vec![10],
        vec![-16],
        vec![]
    ];

    let mut index = 0;
    for arr in test_arrays.iter() {

        let mut result = arr.clone();
        let val = test_values[index];
        move_to_end(&mut result, val);
        if result != expected[index] {
            println!(
                "Ошибка! Входные параметры:\n\
                - массив — {:?}\n\
                - значение — {}\n\
                Результат:\n\
                - ожидаемый — {:?}\n\
                - полученный — {:?}",
                arr, val, expected[index], result);
            return Err(TestResult::WrongResult);
        }

        index += 1;
    }

    Ok(())
}
